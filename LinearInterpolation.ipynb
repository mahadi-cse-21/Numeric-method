   "source": [
    "def linear_interpolation(x0, y0, x1, y1, x):\n",
    "    return y0 + (x - x0) * (y1 - y0) / (x1 - x0)\n",
    "\n",
    "\n",
    "x0 = 2\n",
    "y0 = 1.4142\n",
    "x1 = 3\n",
    "y1 = 1.7321\n",
    "x = 2.5 \n",
    "\n",
    "y = linear_interpolation(x0, y0, x1, y1, x)\n",
    "print(f\"The interpolated value at x = {x} is y = {y}\")\n"
   ]
  }
 ],
